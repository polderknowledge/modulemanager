{
    "name": "polderknowledge/zend-modulemanager",
    "type": "library",
    "description": "A module manager for Zend Framework which can be used to create configs per domain.",
    "keywords": [
        "polderknowledge",
        "modulemanager",
        "zendframework",
        "domain",
        "hostname"
    ],
    "license": "MIT",
    "homepage": "https://polderknowledge.com",
    "require": {
        "php": "^7.0",
        "zendframework/zend-console": "^2.7",
        "zendframework/zend-eventmanager": "^3.2",
        "zendframework/zend-modulemanager": "^2.8"
    },
    "require-dev": {
        "mikey179/vfsStream": "^1.6",
        "polderknowledge/build-tools": "^2.1",
        "zendframework/zend-http": "^2.8",
        "zendframework/zend-servicemanager": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "PolderKnowledge\\ModuleManager\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PolderKnowledge\\ModuleManagerTest\\": "tests"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "process-timeout": 5000,
        "sort-packages": true
    }
}
